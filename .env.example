# MongoDB connection string (replace with your own Mongo URI)
MONGO_URI=mongodb+srv://<username>:<password>@<cluster-url>/<dbname>?retryWrites=true&w=majority&appName=Cluster0

# Secret key for signing JWT tokens
JWT_SECRET=your_jwt_secret_here

# Backend server port
PORT=5000

# Lahza API integration
# LAHZA_API_KEY=your_lahza_api_key_here  # Optional/unused
LAHZA_SECRET_KEY=your_lahza_secret_key_here
LAHZA_API_URL=https://api.lahza.io/

# Frontend client URL for CORS, redirects, etc.
FRONTEND_URL=http://localhost:3000
